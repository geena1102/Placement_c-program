#include<stdio.h>
#include<math.h>
int checkPrime(int num)
{
    if(num==1||num==0)
    {
        return 0;
    }
    int flag=1;
    for(int i=2;i<sqrt(num);i++)
    {
        if (num%i==0)
        {
            flag=0;
            break;
        }
    }
    if(flag==1)
    {
        return flag ;
    }
 
}

int main()
{
    int num;
    scanf("%d",&num);
    int s=checkPrime(num);
    printf("%d",s);
    
}


